{
  "name": "nodejs_web_app",
  "version": "1.0.0",
  "description": "Node.js",
  "author": "James LaBar",
  "main": "app/server.js",
  "scripts": {
    "start": "node server/app/index.js",
    "eslint-server": "node_modules/.bin/eslint -c server/eslintrc.yaml",
    "eslint-client": "node_modules/.bin/eslint -c client/eslintrc.yaml",
    "compile-server": "node_modules/typescript/bin/tsc --project server",
    "compile-client": "node_modules/typescript/bin/tsc --project client",
    "test-server": "node_modules/mocha/bin/mocha --project server -r ts-node/register server/test/**/*.spec.ts"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "eslint": "^4.6.1",
    "express": "^4.13.3",
    "moment": "^2.20.1",
    "tedious": "^2.1.1",
    "typescript": "^2.5.2",
    "typescript-eslint-parser": "^8.0.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/node": "^8.5.2",
    "chai": "^4.1.2",
    "mocha": "^3.5.3",
    "ts-node": "^4.1.0"
  }
}
