  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/body-parser": "^1.17.1",
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.2.5",
    "@types/express": "^4.17.2",
    "@types/express-xml-bodyparser": "^0.3.1",
    "@types/mocha": "^5.2.7",
    "@types/moment": "^2.13.0",
    "@types/mysql": "^2.15.8",
    "@types/node": "^12.12.11",
