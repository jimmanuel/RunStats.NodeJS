    "timers": "^0.1.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "aws-sdk": "^2.578.0",
    "bunyan": "^1.8.12",
    "express": "^4.17.1",
    "express-xml-bodyparser": "^0.3.0",
    "moment": "^2.24.0",
    "mysql": "^2.17.1",
    "promise-mysql": "^4.1.1",
    "xml2js": "^0.4.22"
  }
